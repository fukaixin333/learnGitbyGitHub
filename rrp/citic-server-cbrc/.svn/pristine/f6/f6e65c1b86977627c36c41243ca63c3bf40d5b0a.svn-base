<binding name="cbrc_queryaccount_req8" package="com.citic.server.cbrc.domain.request">
    <include path="../../../custom/base-binding-cbrc.xml" />
    <mapping class="com.citic.server.cbrc.domain.request.CBRC_QueryRequest" name="FEEDBACKFORM">
        <value style="attribute" name="QQDBS" get-method="getQqdbs" set-method="setQqdbs" usage="optional" />
        <value style="attribute" name="CKZTLB" get-method="getZtlb" set-method="setZtlb" usage="optional" />
        <value style="attribute" name="SQJGDM" get-method="getSqjgdm" set-method="setSqjgdm" usage="optional" />
        <value style="attribute" name="MBJGDM" get-method="getMbjgdm" set-method="setMbjgdm" usage="optional" />
        <value style="attribute" name="CXJSSJ" get-method="getCxjssj" set-method="setCxjssj" usage="optional" format="toDateTime14" />
        <value style="attribute" name="YHZJSJSJ" get-method="getYhzjsjsj" set-method="setYhzjsjsj" usage="optional" format="toDateTime14" />
        <value style="attribute" name="SFXD" get-method="getSfxd" set-method="setSfxd" usage="optional" />
        <collection get-method="getCustomerList" set-method="setCustomerList" usage="optional" create-type="java.util.ArrayList">
            <structure type="com.citic.server.cbrc.domain.request.CBRC_QueryRequest_Customer">
                <collection get-method="getAccountList" set-method="setAccountList" usage="optional" create-type="java.util.ArrayList">
                    <structure type="com.citic.server.cbrc.domain.request.CBRC_QueryRequest_Account" name="ACCOUNT">
                        <structure get-method="getCustomer" set-method="setCustomer" usage="optional">
                            <structure map-as="customer" />
                        </structure>
                        <value style="attribute" name="KH" get-method="getKh" set-method="setKh" usage="optional" />
                        <value style="attribute" name="ZH" get-method="getZh" set-method="setZh" usage="optional" />
                        <value style="attribute" name="ZHLB" get-method="getZhlb" set-method="setZhlb" usage="optional" />
                        <value style="attribute" name="ZHZT" get-method="getZhzt" set-method="setZhzt" usage="optional" />
                        <value style="attribute" name="KHWD" get-method="getKhwd" set-method="setKhwd" usage="optional" />
                        <value style="attribute" name="KHWDDM" get-method="getKhwddm" set-method="setKhwddm" usage="optional" />
                        <value style="attribute" name="KHRQ" get-method="getKhrq" set-method="setKhrq" usage="optional" format="toDate8" />
                        <value style="attribute" name="XHRQ" get-method="getXhrq" set-method="setXhrq" usage="optional" format="toDate8" />
                        <value style="attribute" name="XHWD" get-method="getXhwd" set-method="setXhwd" usage="optional" />
                        <value style="attribute" name="BZ" get-method="getBz" set-method="setBz" usage="optional" />
                        <value style="attribute" name="CHBZ" get-method="getChbz" set-method="setChbz" usage="optional" />
                        <value style="attribute" name="ZHYE" get-method="getZhye" set-method="setZhye" usage="optional" />
                        <!--<value style="attribute" name="KYYE" get-method="getKyye" set-method="setKyye" usage="optional" format="noCurrency" /> -->
                        <value style="attribute" name="ZHJYSJ" get-method="getZhjysj" set-method="setZhjysj" usage="optional" format="toDateTime14" />
                        <value style="attribute" name="BEIZ" get-method="getBeiz" set-method="setBeiz" usage="optional" />
                    </structure>
                </collection>
            </structure>
        </collection>
    </mapping>

    <mapping abstract="true" type-name="customer" class="com.citic.server.cbrc.domain.request.CBRC_QueryRequest_Customer">
        <value style="attribute" name="RWLSH" get-method="getRwlsh" set-method="setRwlsh" usage="optional" />
        <value style="attribute" name="CXFKJG" get-method="getCxfkjg" set-method="setCxfkjg" usage="optional" />
        <value style="attribute" name="CXFKJGYY" get-method="getCxfkjgyy" set-method="setCxfkjgyy" usage="optional" />
        <value style="attribute" name="ZZLXDM" get-method="getZzlx" set-method="setZzlx" usage="required" format="noBlank" />
        <value style="attribute" name="ZZHM" get-method="getZzhm" set-method="setZzhm" usage="required" format="noBlank" />
        <value style="attribute" name="KHMC" get-method="getKhmc" set-method="setKhmc" usage="optional" />
        <value style="attribute" name="LXDH" get-method="getLxdh" set-method="setLxdh" usage="optional" />
        <value style="attribute" name="LXSJ" get-method="getLxsj" set-method="setLxsj" usage="optional" />
        <value style="attribute" name="DBRXM" get-method="getDbrxm" set-method="setDbrxm" usage="optional" />
        <value style="attribute" name="DBRZJLX" get-method="getDbrzjlx" set-method="setDbrzjlx" usage="optional" />
        <value style="attribute" name="DBRZJHM" get-method="getDbrzjhm" set-method="setDbrzjhm" usage="optional" />
        <value style="attribute" name="ZZDZ" get-method="getZzdz" set-method="setZzdz" usage="optional" />
        <value style="attribute" name="ZZDH" get-method="getZzdh" set-method="setZzdh" usage="optional" />
        <value style="attribute" name="GZDW" get-method="getGzdw" set-method="setGzdw" usage="optional" />
        <value style="attribute" name="DWDZ" get-method="getDwdz" set-method="setDwdz" usage="optional" />
        <value style="attribute" name="DWDH" get-method="getDwdh" set-method="setDwdh" usage="optional" />
        <value style="attribute" name="YXDZ" get-method="getYxdz" set-method="setYxdz" usage="optional" />
        <value style="attribute" name="FRDB" get-method="getFrdb" set-method="setFrdb" usage="optional" />
        <value style="attribute" name="FRDBZJLX" get-method="getFrdbzjlx" set-method="setFrdbzjlx" usage="optional" />
        <value style="attribute" name="FRDBZJHM" get-method="getFrdbzjhm" set-method="setFrdbzjhm" usage="optional" />
        <value style="attribute" name="KHGSZZHM" get-method="getKhgszzhm" set-method="setKhgszzhm" usage="optional" />
        <value style="attribute" name="GSNSH" get-method="getGsnsh" set-method="setGsnsh" usage="optional" />
        <value style="attribute" name="DSNSH" get-method="getDsnsh" set-method="setDsnsh" usage="optional" />
    </mapping>
</binding>