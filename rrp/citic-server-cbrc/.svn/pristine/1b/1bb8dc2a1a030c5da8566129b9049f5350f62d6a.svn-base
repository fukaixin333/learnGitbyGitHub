<binding name="cbrc_returnreceipt_req4" package="com.citic.server.cbrc.domain">
    <include path="../../../custom/base-binding-cbrc.xml" />
    <mapping class="com.citic.server.cbrc.domain.CBRC_ReturnReceipt" name="RETURNRECEIPTS">
        <structure name="RETURNRECEIPT">
            <value style="attribute" name="QQDBS" get-method="getQqdbs" set-method="setQqdbs" usage="optional" />
            <value style="attribute" name="SQJGDM" get-method="getSqjgdm" set-method="setSqjgdm" usage="optional" />
            <value style="attribute" name="MBJGDM" get-method="getMbjgdm" set-method="setMbjgdm" usage="optional" />
            <value style="attribute" name="HZDM" get-method="getHzdm" set-method="setHzdm" usage="optional" />
            <value style="attribute" name="JSSJ" get-method="getJssj" set-method="setJssj" usage="required" format="toDateTime14" />
            <value style="attribute" name="HZSM" get-method="getHzsm" set-method="setHzsm" usage="required" format="required" />
        </structure>
    </mapping>
</binding>