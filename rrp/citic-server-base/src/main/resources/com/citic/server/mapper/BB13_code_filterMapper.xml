<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.citic.server.mapper.BB13_code_filterMapper">

	<select id="getBB13_code_filterList" parameterType="String" resultType="BB13_code_filter">
		<![CDATA[
		select code, value
		from bb13_code_filter where flag = '1' 
		]]>
	</select>
	
</mapper>
  

