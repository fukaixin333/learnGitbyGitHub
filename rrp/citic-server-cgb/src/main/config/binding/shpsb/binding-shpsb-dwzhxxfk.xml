<!-- 银行提供单位账户信息查询请求 -->
<binding name="binding_shpsb_dwzhxxfk" package="com.citic.server.shpsb.domain.request">
	<include path="../../custom/base-binding-project.xml" />
	<mapping class="com.citic.server.shpsb.domain.request.ShpsbRequestZhxx" name="dwzhxxfk">
		<!-- 首记录 -->
		<structure get-method="getSjl" set-method="setSjl" usage="optional" name="sjl">
			<!-- 公安查询请求总记录数 -->
			<value style="element" name="count" get-method="getCount" set-method="setCount" usage="optional" />
			<!-- 银行代码（见银行代码表） -->
			<value style="element" name="yhdm" get-method="getYhdm" set-method="setYhdm" usage="optional" />
			<!-- 操作时间（银行生成xml文件时间） -->
			<value style="element" name="czsj" get-method="getCzsj" set-method="setCzsj" usage="optional" format="toDate8" />
		</structure>
		<!-- 明细记录 -->
		<collection name="mxjl" get-method="getMxjlList" set-method="setMxjlList" usage="optional" create-type="java.util.ArrayList">
			<!-- 涉案对象（多个） -->
			<structure type="com.citic.server.shpsb.domain.request.ShpsbRequestZhxxMxjl" name="sadx">
				<structure get-method="getSadx" set-method="setSadx" usage="optional">
					<!-- 案号 -->
					<value style="element" name="ah" get-method="getAh" set-method="setAh" usage="optional" />
					<!-- 承办单位 -->
					<value style="element" name="cbdw" get-method="getCbdw" set-method="setCbdw" usage="optional" />
					<!-- 承办人1 -->
					<value style="element" name="cbr1" get-method="getCbr1" set-method="setCbr1" usage="optional" />
					<!-- 承办人2 -->
					<value style="element" name="cbr2" get-method="getCbr2" set-method="setCbr2" usage="optional" />
					<!-- 证照类型 -->
					<value style="element" name="zzlx" get-method="getZzlx" set-method="setZzlx" usage="optional" />
					<!-- 证照号码 -->
					<value style="element" name="zzhm" get-method="getZzhm" set-method="setZzhm" usage="optional" />
					<!-- 涉案对象名称 -->
					<value style="element" name="mc" get-method="getMc" set-method="setMc" usage="optional" />
					<!-- 下面内容为银行填写 -->
					<!-- 处理结果 -->
					<value style="element" name="cljg" get-method="getCljg" set-method="setCljg" usage="optional" />
					<!-- 失败原因 -->
					<value style="element" name="sbyy" get-method="getSbyy" set-method="setSbyy" usage="optional" />
				</structure>
				<!-- 账号信息，可能多个账号信息 -->
				<!-- 若处理结果为失败或零数据，下面内容不填写，若为成功，则无论各项目是否为空均需要按照样例格式完整填写 -->
				<collection get-method="getZhxxList" set-method="setZhxxList" usage="optional" create-type="java.util.ArrayList">
					<structure type="com.citic.server.shpsb.domain.request.ShpsbRequestZhxxMx" name="zhxx">
						<!-- 账（卡）号 -->
						<value style="element" name="zh" get-method="getZh" set-method="setZh" usage="required" format="required" />
						<!-- 帐户状态 -->
						<value style="element" name="zhzt" get-method="getZhzt" set-method="setZhzt" usage="required" format="required" />
						<!-- 开户银行名称 -->
						<value style="element" name="yhmc" get-method="getYhmc" set-method="setYhmc" usage="required" format="required" />
						<!-- 查询时间，在银行系统中查询账户信息的截止时间 -->
						<value style="element" name="cxsj" get-method="getCxsj" set-method="setCxsj" usage="required" format="toDate8Required" />
						<!-- 币种 -->
						<value style="element" name="hbzl" get-method="getHbzl" set-method="setHbzl" usage="required" format="required" />
						<!-- 帐户余额 -->
						<value style="element" name="zhye" get-method="getZhye" set-method="setZhye" usage="required" format="required" />
						<!-- 可用余额 -->
						<value style="element" name="kyye" get-method="getKyye" set-method="setKyye" usage="required" format="required" />
						<!-- 开户日期 -->
						<value style="element" name="khrq" get-method="getKhrq" set-method="setKhrq" usage="required" format="toDate8Required" />
						<!-- 销户日期 -->
						<value style="element" name="xhrq" get-method="getXhrq" set-method="setXhrq" usage="required" format="toDate8Required" />
						<!-- 账户类别 -->
						<value style="element" name="zhlb" get-method="getZhlb" set-method="setZhlb" usage="required" format="required" />
						<!-- 账户序号，银行实际账户序号 -->
						<value style="element" name="zhxh" get-method="getZhxh" set-method="setZhxh" usage="required" format="required" />
					</structure>
				</collection>
			</structure>
		</collection>
	</mapping>
</binding>