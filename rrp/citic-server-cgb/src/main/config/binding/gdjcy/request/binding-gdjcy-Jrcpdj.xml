<binding name="binding_gdjcy_Jrcpdj" package="com.citic.server.gdjg.domain.request">
	<include path="../../../custom/base-binding-project.xml" />
	<mapping class="com.citic.server.gdjg.domain.request.Gdjg_RequestJrcpdj" name="RESULT">
		<structure type="com.citic.server.gdjg.domain.Gdjg_Request" name="COMMAND">
			<value style="attribute" name="TYPE" get-method="getCmdtype" set-method="setCmdtype" usage="optional" />
			<value style="element" name="USERNAME" get-method="getUsername" set-method="setUsername" usage="optional" />
			<value style="element" name="PASSWORD" get-method="getPassword" set-method="setPassword" usage="optional" />
		</structure>
		<structure name="DATA">
			<value style="attribute" name="CONTENT" get-method="getContent" set-method="setContent" usage="optional" />
			<value style="element" name="DOCNO" get-method="getDocno" set-method="setDocno" usage="optional" />
			<collection get-method="getCases" set-method="setCases" usage="optional" create-type="java.util.ArrayList">
				<structure type="com.citic.server.gdjg.domain.request.Gdjg_Request_CaseCx" name="CASE">
					<value style="element" name="CASENO" get-method="getCaseno" set-method="setCaseno" usage="optional" />
					<collection get-method="getRespondents" set-method="setRespondents" usage="optional" create-type="java.util.ArrayList">
						<structure type="com.citic.server.gdjg.domain.request.Gdjg_Request_RespondentCx" name="RESPONDENT">
							<value style="element" name="UNIQUEID" get-method="getUniqueid" set-method="setUniqueid" usage="optional" />
							<value style="element" name="QUERYENDTIME" get-method="getQueryendtime" set-method="setQueryendtime" usage="optional" />
							<value style="element" name="QUERYRESULT" get-method="getQueryresult" set-method="setQueryresult" usage="optional" />
							<value style="element" name="REASON" get-method="getReason" set-method="setReason" usage="optional" />
							<value style="element" name="QUERYTIME" get-method="getQuerytime" set-method="setQuerytime" usage="optional" />
							<collection get-method="getBanks" set-method="setBanks" usage="optional" create-type="java.util.ArrayList">
								<structure type="com.citic.server.gdjg.domain.request.Gdjg_Request_BankCx" name="BANK">
									<value style="element" name="BANKCODE" get-method="getBankcode" set-method="setBankcode" usage="optional" />
									<value style="element" name="BANKNAME" get-method="getBankname" set-method="setBankname" usage="optional" />
									<collection get-method="getFinancialPros" set-method="setFinancialPros" usage="optional" create-type="java.util.ArrayList">
										<structure type="com.citic.server.gdjg.domain.request.Gdjg_Request_FinancialProductsCx" name="FINANCIALPRODUCTS">
											<value style="element" name="PRODUCTYPE" get-method="getProducttype" set-method="setProducttype" usage="optional" />
											<value style="element" name="ORGNAME" get-method="getOrgname" set-method="setOrgname" usage="optional" />
											<value style="element" name="PRODUCTCODE" get-method="getProductcode" set-method="setProductcode" usage="optional" />
											<value style="element" name="PRODUCTNAME" get-method="getProductname" set-method="setProductname" usage="optional" />
											<value style="element" name="CURRENCY" get-method="getCurrency" set-method="setCurrency" usage="optional" />
											<value style="element" name="BANLANCE" get-method="getBanlance" set-method="setBanlance" usage="optional" format="noCurrency"/>
											<value style="element" name="SALESTYPE" get-method="getSalestype" set-method="setSalestype" usage="optional" />
											<value style="element" name="PRODUCTSUM" get-method="getProductsum" set-method="setProductsum" usage="optional" />
											<value style="element" name="PRODUCTREMARK" get-method="getProductremark" set-method="setProductremark" usage="optional" />
										</structure>
									</collection>
								</structure>
							</collection>
						</structure>
					</collection>
				</structure>
			</collection>
		</structure>
	</mapping>
</binding>