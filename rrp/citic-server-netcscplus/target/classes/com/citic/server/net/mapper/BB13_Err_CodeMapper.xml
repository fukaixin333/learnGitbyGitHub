<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.citic.server.net.mapper.BB13_Err_CodeMapper">
    <select id="selectAllErrCode" parameterType="String" resultType="com.citic.server.inner.domain.AnswerCode">
        SELECT E.Scode, E.Tcode, E.Descr, E.Type FROM bb13_err_code_map E
    </select>

    <select id="selectErrCodeBySCode" parameterType="String" resultType="com.citic.server.inner.domain.AnswerCode">
        SELECT E.Scode, E.Tcode, E.Descr, E.Type FROM bb13_err_code_map E WHERE E.Scode = #{scode} and E.Type = #{type}
    </select>
</mapper>