<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.citic.server.mapper.MC00_taskMapper">

	<select id="getMC00_taskList" resultType="MC00_task">
		<![CDATA[
		select taskid,tgroupid,trigerid,tasksplit,splitparams,taskcmd,taskname,des,flag
		from mc00_task 
		where flag = '1'
		]]>
	</select>
	
</mapper>
  

