<!-- 银行提供单位开户资料查询反馈 -->
<binding name="binding_shpsb_dwkhzlfk" package="com.citic.server.shpsb.domain.request">
	<include path="../../custom/base-binding-project.xml" />
	<mapping class="com.citic.server.shpsb.domain.request.ShpsbRequestKhzl" name="dwkhzlfk">
		<!-- 首记录 -->
		<structure get-method="getSjl" set-method="setSjl" usage="optional" name="sjl">
			<!-- 公安查询请求总记录数 -->
			<value style="element" name="count" get-method="getCount" set-method="setCount" usage="optional" />
			<!-- 银行代码（见银行代码表） -->
			<value style="element" name="yhdm" get-method="getYhdm" set-method="setYhdm" usage="optional" />
			<!-- 操作时间（银行生成xml文件时间） -->
			<value style="element" name="czsj" get-method="getCzsj" set-method="setCzsj" usage="optional" format="toDate8" />
		</structure>
		<!-- 明细记录 -->
		<collection get-method="getMxjlList" set-method="setMxjlList" usage="optional" create-type="java.util.ArrayList" name="mxjl">
			<!-- 涉案对象（多个） -->
			<structure type="com.citic.server.shpsb.domain.request.ShpsbRequestKhzlMxjl" name="sadx">
				<structure get-method="getSadx" set-method="setSadx" usage="optional">
					<!-- 案号 -->
					<value style="element" name="ah" get-method="getAh" set-method="setAh" usage="optional" />
					<!-- 承办单位 -->
					<value style="element" name="cbdw" get-method="getCbdw" set-method="setCbdw" usage="optional" />
					<!-- 承办人1 -->
					<value style="element" name="cbr1" get-method="getCbr1" set-method="setCbr1" usage="optional" />
					<!-- 承办人2 -->
					<value style="element" name="cbr2" get-method="getCbr2" set-method="setCbr2" usage="optional" />
					<!-- 证照类型 -->
					<value style="element" name="zzlx" get-method="getZzlx" set-method="setZzlx" usage="optional" />
					<!-- 证照号码 -->
					<value style="element" name="zzhm" get-method="getZzhm" set-method="setZzhm" usage="optional" />
					<!-- 涉案对象名称 -->
					<value style="element" name="mc" get-method="getMc" set-method="setMc" usage="optional" />
					<!-- 下面内容为银行填写 -->
					<!-- 处理结果 -->
					<value style="element" name="cljg" get-method="getCljg" set-method="setCljg" usage="optional" />
					<!-- 失败原因 -->
					<value style="element" name="sbyy" get-method="getSbyy" set-method="setSbyy" usage="optional" />
				</structure>
				<!-- 开户资料 -->
				<!-- 若处理结果为失败或零数据，下面内容不填写，若为成功，则无论各项目是否为空均需要按照样例格式完整填写 -->
				<collection get-method="getKhzlList" set-method="setKhzlList" usage="optional" create-type="java.util.ArrayList">
					<structure type="com.citic.server.shpsb.domain.request.ShpsbRequestKhzlMx" name="khzl">
						<!-- 地址 -->
						<value style="element" name="dz" get-method="getDwdz" set-method="setDwdz" usage="required" format="required" />
						<!-- 电话 -->
						<value style="element" name="dh" get-method="getDwdh" set-method="setDwdh" usage="required" format="required" />
						<!-- 法人代表 -->
						<value style="element" name="frdb" get-method="getFrdb" set-method="setFrdb" usage="required" format="required" />
						<!-- 法人证件类型 -->
						<value style="element" name="frdbzjlx" get-method="getFrdbzzlx" set-method="setFrdbzzlx" usage="required" format="required" />
						<!-- 法人证件号码 -->
						<value style="element" name="frdbzjhm" get-method="getFrdbzzhm" set-method="setFrdbzzhm" usage="required" format="required" />
						<!-- 联系人 -->
						<value style="element" name="lxr" get-method="getLxr" set-method="setLxr" usage="required" format="required" />
						<!-- E_MAIL地址 -->
						<value style="element" name="email" get-method="getEmail" set-method="setEmail" usage="required" format="required" />
						<!-- 代办人姓名 -->
						<value style="element" name="dbrxm" get-method="getDbrxm" set-method="setDbrxm" usage="required" format="required" />
						<!-- 代办人证件类型 -->
						<value style="element" name="dbrzzlx" get-method="getDbrzzlx" set-method="setDbrzzlx" usage="required" format="required" />
						<!-- 代办人证件号码 -->
						<value style="element" name="dbrzzh" get-method="getDbrzzh" set-method="setDbrzzh" usage="required" format="required" />
						<!-- 社会同一信用代码 -->
						<value style="element" name="shxydm" get-method="getShxydm" set-method="setShxydm" usage="required" format="required" />
					</structure>
				</collection>
			</structure>
		</collection>
	</mapping>
</binding>