<!-- 银行提供交个人操作日志查询反馈 -->
<binding name="binding_shpsb_grczrzfk" package="com.citic.server.shpsb.domain.request">
    <include path="../../custom/base-binding-project.xml" />
    <mapping class="com.citic.server.shpsb.domain.request.ShpsbRequestCzrz" name="grczrzfk">
        <!-- 首记录 -->
        <structure get-method="getSjl" set-method="setSjl" usage="optional" name="sjl">
            <!-- 银行查询总记录数 -->
            <value style="element" name="count" get-method="getCount" set-method="setCount" usage="optional" />
            <!-- 银行代码（见银行代码表），公安已填写，银行无需填写 -->
            <value style="element" name="yhdm" get-method="getYhdm" set-method="setYhdm" usage="optional" />
            <!-- 操作时间（银行生成xml文件时间） -->
            <value style="element" name="czsj" get-method="getCzsj" set-method="setCzsj" usage="optional" format="toDate8" />
        </structure>
        <!-- 明细记录 -->
        <collection get-method="getMxjlList" set-method="setMxjlList" usage="optional" create-type="java.util.ArrayList" name="mxjl">
            <!-- 涉案对象（多个） -->
            <structure type="com.citic.server.shpsb.domain.request.ShpsbRequestCzrzMxjl" name="sadx">
                <structure get-method="getSadx" set-method="setSadx" usage="optional">
                    <!-- 下面内容为公安已填写内容，银行无需填写，沿用查询请求文件中内容 -->
                    <!-- 案号 -->
                    <value style="element" name="ah" get-method="getAh" set-method="setAh" usage="optional" />
                    <!-- 承办单位名称 -->
                    <value style="element" name="cbdw" get-method="getCbdw" set-method="setCbdw" usage="optional" />
                    <!-- 承办人1 -->
                    <value style="element" name="cbr1" get-method="getCbr1" set-method="setCbr1" usage="optional" />
                    <!-- 承办人2 -->
                    <value style="element" name="cbr2" get-method="getCbr2" set-method="setCbr2" usage="optional" />
                    <!-- 条件类别 1 按账卡号查询：账(卡)号、查询时间（起）、查询时间（止）必填 2 按交易流水号查询：交易流水号必填 -->
                    <value style="element" name="tjlb" get-method="getTjlb" set-method="setTjlb" usage="optional" />
                    <!-- 账(卡)号 -->
                    <value style="element" name="zh" get-method="getZh" set-method="setZh" usage="optional" />
                    <!-- 交易流水号 -->
                    <value style="element" name="jylsh" get-method="getJylsh" set-method="setJylsh" usage="optional" />
                    <!-- 查询时间（起） -->
                    <value style="element" name="cxkssj" get-method="getCxkssj" set-method="setCxkssj" usage="optional" format="toDate8" />
                    <!-- 查询时间（止） -->
                    <value style="element" name="cxjssj" get-method="getCxjssj" set-method="setCxjssj" usage="optional" format="toDate8" />
                    <!-- 下面内容为银行填写 -->
                    <!-- 处理结果，必须填写 -->
                    <value style="element" name="cljg" get-method="getCljg" set-method="setCljg" usage="optional" />
                    <!-- 失败原因 -->
                    <value style="element" name="sbyy" get-method="getSbyy" set-method="setSbyy" usage="optional" />
                </structure>
                <!-- 若处理结果为失败，下面内容不填写，若为成功，则无论各项目是否为空均需要按照样例格式完整填写 -->
                <!-- 操作日志，可能多个操作日志 -->
                <collection get-method="getCzrzList" set-method="setCzrzList" usage="optional" create-type="java.util.ArrayList">
                    <structure type="com.citic.server.shpsb.domain.request.ShpsbRequestCzrzMx" name="czrz">
                        <!-- IP地址 -->
                        <value style="element" name="ip" get-method="getIp" set-method="setIp" usage="optional" />
                        <!-- MAC地址 -->
                        <value style="element" name="mac" get-method="getMac" set-method="setMac" usage="optional" />
                        <!-- 操作类型 -->
                        <value style="element" name="czlx" get-method="getCzlx" set-method="setCzlx" usage="optional" />
                        <!-- 操作网点 -->
                        <value style="element" name="czwd" get-method="getCzwd" set-method="setCzwd" usage="optional" />
                        <!-- 操作开始时间 -->
                        <value style="element" name="czkssj" get-method="getCzkssj" set-method="setCzkssj" usage="optional" format="toDateTime14" />
                        <!-- 操作结束时间 -->
                        <value style="element" name="czjssj" get-method="getCzjssj" set-method="setCzjssj" usage="optional" format="toDateTime14" />
                        <!-- 经度 -->
                        <value style="element" name="lng" get-method="getLng" set-method="setLng" usage="optional" />
                        <!-- 纬度 -->
                        <value style="element" name="lat" get-method="getLat" set-method="setLat" usage="optional" />
                    </structure>
                </collection>
            </structure>
        </collection>
    </mapping>
</binding>