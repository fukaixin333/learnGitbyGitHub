<binding name="binding_response" package="com.citic.server.gf.domain">
    <mapping name="result" class="com.citic.server.gf.domain.Result">
        <value style="attribute" name="errorMsg" get-method="getErrorMsg" set-method="setErrorMsg" usage="optional" />
        <value style="attribute" name="errMsg" get-method="getErrMsg" set-method="setErrMsg" usage="optional" />
        <collection name="cxjglist" get-method="getCxJgList" set-method="setCxJgList" usage="optional" create-type="java.util.ArrayList">
            <structure type="com.citic.server.gf.domain.Result_Jg" name="result">
                <structure map-as="resultJg" />
            </structure>
        </collection>
        <collection name="htjglist" get-method="getHtJgList" set-method="setHtJgList" usage="optional" create-type="java.util.ArrayList">
            <structure type="com.citic.server.gf.domain.Result_Jg" name="result">
                <structure map-as="resultJg" />
            </structure>
        </collection>
    </mapping>
    <mapping abstract="true" type-name="resultJg" class="com.citic.server.gf.domain.Result_Jg">
        <value style="attribute" name="BDHM" get-method="getBdhm" set-method="setBdhm" usage="optional" />
        <value style="attribute" name="RESULT" get-method="getResult" set-method="setResult" usage="optional" />
        <value style="attribute" name="MSG" get-method="getMsg" set-method="setMsg" usage="optional" />
    </mapping>
</binding>