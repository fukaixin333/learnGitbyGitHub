package com.citic.server.dx.domain;

import java.io.Serializable;

/**
 * 响应报文
 * 
 * @author Liu Xuanfei
 * @date 2016年4月14日 下午5:41:08
 */
public class Response implements Serializable {
	private static final long serialVersionUID = -294174978742693621L;
	
	/** 响应报文 */
	private String responseXML;
	
	/** 返回码 */
	private String code = "";
	
	/** 传输报文流水号 */
	private String transSerialNumber = "";
	
	/** 返回消息 */
	private String description = "";
	
	private String applicationId;
	
	// ==========================================================================================
	//                     Help Field
	// ==========================================================================================
	/** 反馈时间 */
	private String feedback_dt = "";
	
	// ==========================================================================================
	//                     Getter and Setter
	// ==========================================================================================
	public String getResponseXML() {
		return responseXML;
	}
	
	public void setResponseXML(String responseXML) {
		this.responseXML = responseXML;
	}
	
	public String getCode() {
		return code;
	}
	
	public void setCode(String code) {
		this.code = code;
	}
	
	public String getTransSerialNumber() {
		return transSerialNumber;
	}
	
	public void setTransSerialNumber(String transSerialNumber) {
		this.transSerialNumber = transSerialNumber;
	}
	
	public String getDescription() {
		return description;
	}
	
	public void setDescription(String description) {
		this.description = description;
	}
	
	public String getFeedback_dt() {
		return feedback_dt;
	}
	
	public void setFeedback_dt(String feedback_dt) {
		this.feedback_dt = feedback_dt;
	}
	
	public String getApplicationId() {
		return applicationId;
	}
	
	public void setApplicationId(String applicationId) {
		this.applicationId = applicationId;
	}
}
