<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 
<mapper namespace="com.citic.server.net.mapper.MM24_q_attachMapper"> 
 
 
  <insert id="insertBg_q_attach" parameterType="com.citic.server.dx.domain.Bg_Q_Attach"> 
    insert into Bg_q_attach(
     fileno,transserialnumber,filename,filepath,create_dt
    ) values(
    #{fileno},#{transserialnumber},#{filename},#{filepath},#{create_dt}
    ) 
    </insert> 
 
 
  <delete id="delBg_q_attach" parameterType="String">  
  <![CDATA[ delete from Bg_q_attach where transserialnumber=#{transserialnumber} ]]>
   </delete> 
 
  
 
</mapper> 
